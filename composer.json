{
    "name": "uthereal/digitalocean-pricer",
    "type": "project",
    "description": "DigitalOcean pricer is a tool used to gain insight into the various costs associated with your infrastructure.",
    "keywords": ["uthereal", "digitalocean", "price", "tracker"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel-zero/framework": "^9.0",
        "illuminate/http": "^9.0",
        "nunomaduro/termwind": "^1.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "builds/do-pricer"
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
